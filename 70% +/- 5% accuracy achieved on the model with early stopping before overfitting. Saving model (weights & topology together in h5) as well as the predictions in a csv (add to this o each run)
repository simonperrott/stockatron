        "amzn = yt.Ticker('AMZN')\n",
        "start_date = '1999-01-01'\n",
        "df = amzn.history(start=start_date)\n",
        "df.drop(['Dividends', 'Stock Splits', 'High', 'Low'], axis=1, inplace=True)\n",
